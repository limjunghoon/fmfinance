<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical" android:layout_height="wrap_content" android:layout_width="fill_parent">

		<LinearLayout android:id="@+id/LinearLayout02" android:layout_height="wrap_content" android:layout_width="fill_parent" android:gravity="center_horizontal" android:visibility="gone">
			<TextView android:layout_height="wrap_content" android:id="@+id/TVTotalAssetsAmount" android:textSize="18sp" android:layout_width="wrap_content" android:layout_marginRight="3dp"></TextView>
	//		<TextView android:layout_height="wrap_content" android:id="@+id/TVPercentAssetsAmount" android:textSize="18sp" android:layout_width="wrap_content" android:layout_marginLeft="3dp"></TextView>
		</LinearLayout>
		<LinearLayout android:id="@+id/LinearLayout01" android:layout_height="wrap_content" android:layout_width="fill_parent" android:gravity="center" android:visibility="gone">
	   		<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/BtnPreviusYear" android:text="이전년">
	   		</Button>
	   		<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="년도" android:textSize="20sp" android:id="@+id/TVCurrentYear" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
	   		</TextView>
	   		<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/BtnNextYear" android:text="다음년">
	   		</Button>
		

		</LinearLayout>
		<HorizontalScrollView android:id="@+id/HorizontalScrollView01" android:layout_height="wrap_content" android:layout_width="fill_parent">
			<LinearLayout android:id="@+id/hsv" android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content">
				<com.fletamuto.common.control.fmgraph.LineGraph android:id="@+id/lgraph" android:layout_width="210dp" android:layout_height="200dp" />
			</LinearLayout>
		</HorizontalScrollView>
		<LinearLayout android:id="@+id/LinearLayout01" android:layout_width="fill_parent" android:layout_height="fill_parent" android:orientation="vertical">
		<TextView android:layout_height="wrap_content" android:id="@+id/TVMonth" android:gravity="center" android:textSize="18sp" android:layout_width="fill_parent" android:visibility="gone"></TextView>
		<ListView android:id="@+id/LVMonthItem" android:layout_width="fill_parent" android:layout_height="fill_parent"></ListView>
	</LinearLayout>

</LinearLayout>
