<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
   	android:scrollbarStyle="outsideOverlay">
   	
	<ScrollView android:id="@+id/ScrollView01" android:layout_width="fill_parent" android:layout_height="fill_parent">
		<LinearLayout android:layout_width="fill_parent" android:layout_height="fill_parent" android:orientation="vertical" android:id="@+id/LLSettleAccounts">
			<LinearLayout android:id="@+id/LinearLayout02" android:layout_height="wrap_content" android:layout_width="fill_parent" android:orientation="vertical" android:gravity="center">
			<TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:textSize="20sp" android:id="@+id/TVSettleDate" android:gravity="center"></TextView></LinearLayout>
			<HorizontalScrollView android:id="@+id/scrollview" android:layout_width="wrap_content" android:layout_height="wrap_content" android:scrollbarStyle="outsideOverlay">
	   			<LinearLayout android:id="@+id/hsv" android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content">
	       			<com.fletamuto.common.control.fmgraph.BarGraph android:id="@+id/bgraph" android:layout_height="150dip" android:layout_width="fill_parent" />
				</LinearLayout>
   			</HorizontalScrollView>
 			<LinearLayout android:layout_height="wrap_content" android:orientation="vertical" android:layout_width="fill_parent" android:id="@+id/LLSettleIncome" android:background="#505050">
 				<LinearLayout android:id="@+id/LinearLayout01" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingBottom="5dp" android:paddingTop="5dp">
 					<FrameLayout android:layout_height="fill_parent" android:layout_width="20dp">
 						<ImageView android:id="@+id/IVsettleIncomeColor" android:layout_width="10dp" android:layout_height="10dp" android:layout_gravity="center"/>
 					</FrameLayout>
 					<TextView android:layout_height="wrap_content" android:id="@+id/TVSettleIncomeT" android:text="수입" android:layout_width="80dp" android:textSize="18dp">
					</TextView>
					<TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/TVSettleIncomeAmount" android:text="0" android:gravity="right" android:layout_marginRight="50dp" android:textSize="18dp">
					</TextView>
	 			</LinearLayout>
	 			<LinearLayout android:id="@+id/LinearLayout01" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingBottom="5dp" android:paddingTop="5dp">
 					<FrameLayout android:layout_height="fill_parent" android:layout_width="20dp">
 						<ImageView android:id="@+id/IVsettleExpenseColor" android:layout_width="10dp" android:layout_height="10dp" android:layout_gravity="center"/>
 					</FrameLayout>
 					<TextView android:layout_height="wrap_content" android:id="@+id/TVSettleExpenseT" android:text="지출" android:layout_width="80dp" android:textSize="18dp">
					</TextView>
					<TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/TVSettleExpenseAmount" android:text="0" android:gravity="right" android:layout_marginRight="50dp" android:textSize="18dp">
					</TextView>
	 			</LinearLayout>
	 			<LinearLayout android:id="@+id/LinearLayout01" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingBottom="5dp" android:paddingTop="5dp">
 					<FrameLayout android:layout_height="fill_parent" android:layout_width="20dp">
 						<ImageView android:id="@+id/IVsettleIncomeExpenseDifferenceColor" android:layout_width="10dp" android:layout_height="10dp" android:layout_gravity="center" android:visibility="invisible"/>
 					</FrameLayout>
 					<TextView android:layout_height="wrap_content" android:id="@+id/TVSettleIncomeExpenseDifferenceT" android:text="차액" android:layout_width="80dp" android:textSize="18dp">
					</TextView>
					<TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/TVSettleIncomeExpenseDifference" android:text="0" android:gravity="right" android:layout_marginRight="50dp" android:textSize="18dp">
					</TextView>
	 			</LinearLayout>
 			</LinearLayout>
 			<LinearLayout android:layout_height="wrap_content" android:orientation="vertical" android:layout_width="fill_parent" android:layout_marginTop="15dp" android:background="#505050" android:id="@+id/LLSettleBudget">
 				<LinearLayout android:id="@+id/LinearLayout01" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingBottom="5dp" android:paddingTop="5dp">
 					<FrameLayout android:layout_height="fill_parent" android:layout_width="20dp">
 						<ImageView android:id="@+id/IVsettleBudgetColor" android:layout_width="10dp" android:layout_height="10dp" android:layout_gravity="center"/>
 					</FrameLayout>
 					<TextView android:layout_height="wrap_content" android:id="@+id/TVSettleBudgetT" android:text="예산" android:layout_width="80dp" android:textSize="18dp">
					</TextView>
					<TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/TVSettleBudgetAmount" android:text="0" android:gravity="right" android:layout_marginRight="50dp" android:textSize="18dp">
					</TextView>
	 			</LinearLayout>
	 			<LinearLayout android:id="@+id/LinearLayout01" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingBottom="5dp" android:paddingTop="5dp">
 					<FrameLayout android:layout_height="fill_parent" android:layout_width="20dp">
 						<ImageView android:id="@+id/IVsettleBudgetExpenseColor" android:layout_width="10dp" android:layout_height="10dp" android:layout_gravity="center"/>
 					</FrameLayout>
 					<TextView android:layout_height="wrap_content" android:id="@+id/TVSettleBudgetExpenseT" android:text="지출" android:layout_width="80dp" android:textSize="18dp">
					</TextView>
					<TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/TVSettleBudgetExpeseAmount" android:text="0" android:gravity="right" android:layout_marginRight="50dp" android:textSize="18dp">
					</TextView>
	 			</LinearLayout>
	 			<LinearLayout android:id="@+id/LinearLayout01" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingBottom="5dp" android:paddingTop="5dp">
 					<FrameLayout android:layout_height="fill_parent" android:layout_width="20dp">
 						<ImageView android:id="@+id/IVsettleBudgetDifferenceColor" android:layout_width="10dp" android:layout_height="10dp" android:layout_gravity="center" android:visibility="invisible"/>
 					</FrameLayout>
 					<TextView android:layout_height="wrap_content" android:id="@+id/TVSettleBudgetDifferenceT" android:layout_width="80dp" android:textSize="18dp" android:text="차액">
					</TextView>
					<TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/TVSettleBudgetDifference" android:text="0" android:gravity="right" android:layout_marginRight="50dp" android:textSize="18dp">
					</TextView>
	 			</LinearLayout>
 			</LinearLayout>
 			<LinearLayout android:layout_height="wrap_content" android:orientation="vertical" android:layout_width="fill_parent" android:layout_marginTop="15dp" android:background="#505050" android:id="@+id/LLSettleAssets">
	 			<LinearLayout android:id="@+id/LinearLayout01" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingBottom="5dp" android:paddingTop="5dp">
 					<FrameLayout android:layout_height="fill_parent" android:layout_width="20dp">
 						<ImageView android:id="@+id/IVsettleAssetsColor" android:layout_width="10dp" android:layout_height="10dp" android:layout_gravity="center"/>
 					</FrameLayout>
 					<TextView android:layout_height="wrap_content" android:id="@+id/TVSettleAssetsT" android:text="자산" android:layout_width="80dp" android:textSize="18dp">
					</TextView>
					<TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/TVSettleAssetsAmount" android:text="0" android:gravity="right" android:layout_marginRight="50dp" android:textSize="18dp">
					</TextView>
	 			</LinearLayout>
	 			<LinearLayout android:id="@+id/LinearLayout01" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingBottom="5dp" android:paddingTop="5dp">
 					<FrameLayout android:layout_height="fill_parent" android:layout_width="20dp">
 						<ImageView android:id="@+id/IVsettleLiabilityColor" android:layout_width="10dp" android:layout_height="10dp" android:layout_gravity="center"/>
 					</FrameLayout>
 					<TextView android:layout_height="wrap_content" android:id="@+id/TVSettleLiabilityT" android:text="부채" android:layout_width="80dp" android:textSize="18dp">
					</TextView>
					<TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/TVSettleLiabilityAmount" android:text="0" android:gravity="right" android:layout_marginRight="50dp" android:textSize="18dp">
					</TextView>
	 			</LinearLayout>
	 			<LinearLayout android:id="@+id/LinearLayout01" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingBottom="5dp" android:paddingTop="5dp">
 					<FrameLayout android:layout_height="fill_parent" android:layout_width="20dp">
 						<ImageView android:id="@+id/IVsettleAssetsLiabilityDifferenceColor" android:layout_width="10dp" android:layout_height="10dp" android:layout_gravity="center" android:visibility="invisible"/>
 					</FrameLayout>
 					<TextView android:layout_height="wrap_content" android:id="@+id/TVSettleAssetsLiabilityDifferenceT" android:text="차액" android:layout_width="80dp" android:textSize="18dp">
					</TextView>
					<TextView android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/TVSettleAssetsLiabilityDifference" android:text="0" android:gravity="right" android:layout_marginRight="50dp" android:textSize="18dp">
					</TextView>
	 			</LinearLayout>
 			</LinearLayout>
 		
</LinearLayout>


</ScrollView>
	    















</LinearLayout>